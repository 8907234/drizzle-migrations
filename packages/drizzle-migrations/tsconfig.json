{
  "compilerOptions": {
    "allowJs": true,
    "lib": ["esnext"],
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "target": "ESNext"
  },
  "include": ["src"]
}
